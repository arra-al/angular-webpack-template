{
    "name": "webpack-ts",
    "version": "1.0.0",
    "description": "Learning webpack",
    "main": "src/index.ts",
    "scripts": {
        "prettier:format": "prettier --write \"{,src/**/}*.{md,json,ts,css,scss,yml}\"",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "npm run lint -- --fix",
        "start": "npm run webpack:dev",
        "watch": "webpack --watch",
        "postinstall": "node node_modules/protractor/bin/webdriver-manager update --gecko false",
        "e2e": "protractor src/test/protractor.conf.js",
        "test": "jest --coverage --logHeapUsage -w=2 --config src/test/jest.conf.js",
        "test:watch": "npm run test -- --watch",
        "ngc:build": "ngc -p tsconfig-aot.json",
        "tsc:build": "tsc -p tsconfig.json",
        "build:prod": "webpack --config webpack/webpack.prod.js",
        "build": "webpack --config  webpack/webpack.dev.js --env",
        "dbg:build": "node-nightly --inspect --debug-brk ./node_modules/webpack/bin/webpack.js --config webpack/webpack.config.js ",
        "cleanup": "rimraf dist/",
        "clean-www": "rimraf dist/",
        "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --env.stats=minimal",
        "webpack:dev-verbose": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --profile --progress --env.stats=normal",
        "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --env.stats=minimal",
        "webpack:build": "npm run cleanup && npm run webpack:build:main",
        "webpack:prod:main": "npm run webpack -- --config webpack/webpack.prod.js --profile",
        "webpack:prod": "npm run cleanup && npm run webpack:prod:main && npm run clean-www",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
    },
    "author": "Gezim Muja",
    "license": "ISC",
    "dependencies": {
        "@angular/animations": "^8.2.14",
        "@angular/common": "^8.2.14",
        "@angular/compiler": "^8.2.14",
        "@angular/core": "^8.2.14",
        "@angular/forms": "^8.2.14",
        "@angular/platform-browser": "^8.2.14",
        "@angular/platform-browser-dynamic": "^8.2.14",
        "@angular/router": "^8.2.14",
        "core-js": "^3.6.1",
        "rxjs": "^6.5.4",
        "zone.js": "^0.10.2"
    },
    "devDependencies": {
        "@angular/compiler-cli": "^8.2.14",
        "@ngtools/webpack": "^8.3.21",
        "@types/chai": "^4.2.7",
        "@types/chai-string": "^1.4.2",
        "@types/jest": "^24.0.25",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.1",
        "@types/zone.js": "^0.5.12",
        "@typescript-eslint/eslint-plugin": "^2.15.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.15.0",
        "@typescript-eslint/parser": "^2.15.0",
        "angular2-template-loader": "^0.6.2",
        "autoprefixer": "^9.7.3",
        "base-href-webpack-plugin": "^2.0.0",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cache-loader": "^4.1.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-string": "^1.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.1",
        "eslint": "^6.8.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-prettier": "^3.1.2",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-date-mock": "^1.0.7",
        "jest-junit": "^10.0.0",
        "jest-preset-angular": "^8.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^7.0.0",
        "moment-locales-webpack-plugin": "^1.1.2",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.19.1",
        "protractor": "^5.4.2",
        "sass": "^1.24.1",
        "sass-loader": "^8.0.0",
        "simple-progress-webpack-plugin": "^1.1.2",
        "style-loader": "^1.1.2",
        "terser-webpack-plugin": "^2.3.1",
        "thread-loader": "^2.1.3",
        "to-string-loader": "^1.1.6",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-merge": "^4.2.2",
        "webpack-notifier": "^1.8.0",
        "webpack-visualizer-plugin": "^0.1.11",
        "workbox-webpack-plugin": "^4.3.1",
        "write-file-webpack-plugin": "^4.5.1"
    }
}
